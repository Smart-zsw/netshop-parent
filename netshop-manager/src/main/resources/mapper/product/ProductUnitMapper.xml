<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zsw.netshop.manager.mapper.ProductUnitMapper">

    <select id="findAll" resultType="com.zsw.netshop.model.entity.base.ProductUnit">
        select *
        from product_unit
        where is_deleted = 0
        order by id
    </select>
</mapper>